      "version": "master"
    },
    {
      "name": "contact-form",
      "source": "git@github.com:BloggifyPlugins/contact-form.git",
      "version": "master",
      "config": {
        "mandrill_key": "some mandrill key",
        "contact_page": "/contact",
        "contact": {
