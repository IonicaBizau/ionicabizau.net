      "version": "master"
    },
    {
      "name": "_api",
      "source": "git@github.com:IonicaBizau/api.git",
      "version": "master"
    },
    {
      "name": "_dashboard",
      "source": "git@github.com:IonicaBizau/dashboard.git",
      "version": "master",
      "config": {
        "users": [
          {
            "username": "my username",
            "displayName": "Ionică Bizău",
            "password": "some nice password"
          }
        ]
      }
    },
    {
      "name": "lightbox",
      "source": "git@github.com:BloggifyPlugins/lightbox.git",
      "version": "master"
    },
    {
      "name": "rss",
      "source": "git@github.com:Bloggify/rss.git",
      "version": "master",
